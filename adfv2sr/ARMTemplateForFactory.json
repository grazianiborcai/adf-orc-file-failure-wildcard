{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adfv2sr"
		},
		"integrationRuntime_ZabkaPowerBI-IR": {
			"type": "string",
			"metadata": "Integration Runtime Name",
			"defaultValue": "ZabkaPowerBI-IR"
		},
		"ZabkaBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ZabkaBlob'"
		},
		"ZabkaDW_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ZabkaDW'"
		},
		"Blob_ORC_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "*.orc"
		},
		"Blob_ORC_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "input/orc"
		},
		"Blob_ORC_without_file_name_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"Blob_ORC_without_file_name_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "input/orc"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ORC files failure with wildcard')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "SqlDWSink",
								"allowPolyBase": true,
								"writeBatchSize": 10000,
								"polyBaseSettings": {
									"rejectValue": 0,
									"rejectType": "value",
									"useTypeDefault": true
								}
							},
							"enableStaging": false,
							"dataIntegrationUnits": 2
						},
						"inputs": [
							{
								"referenceName": "Blob_ORC",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "SQLDW_ORC",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Blob_ORC')]",
				"[concat(variables('factoryId'), '/datasets/SQLDW_ORC')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ORC files succeed without wildcard')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "SqlDWSink",
								"allowPolyBase": true,
								"writeBatchSize": 10000,
								"polyBaseSettings": {
									"rejectValue": 0,
									"rejectType": "value",
									"useTypeDefault": true
								}
							},
							"enableStaging": false,
							"dataIntegrationUnits": 2
						},
						"inputs": [
							{
								"referenceName": "Blob_ORC_without_file_name",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "SQLDW_ORC",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Blob_ORC_without_file_name')]",
				"[concat(variables('factoryId'), '/datasets/SQLDW_ORC')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Blob_ORC')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ZabkaBlob",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureBlob",
				"typeProperties": {
					"format": {
						"type": "OrcFormat"
					},
					"fileName": "[parameters('Blob_ORC_properties_typeProperties_fileName')]",
					"folderPath": "[parameters('Blob_ORC_properties_typeProperties_folderPath')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ZabkaBlob')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Blob_ORC_without_file_name')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ZabkaBlob",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureBlob",
				"typeProperties": {
					"format": {
						"type": "OrcFormat"
					},
					"fileName": "[parameters('Blob_ORC_without_file_name_properties_typeProperties_fileName')]",
					"folderPath": "[parameters('Blob_ORC_without_file_name_properties_typeProperties_folderPath')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ZabkaBlob')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SQLDW_ORC')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ZabkaDW",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlDWTable",
				"typeProperties": {
					"tableName": "[concat('[', 'dbo].[table_orc]')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ZabkaDW')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ZabkaBlob')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('ZabkaBlob_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ZabkaDW')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDW",
				"typeProperties": {
					"connectionString": "[parameters('ZabkaDW_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/', parameters('integrationRuntime_ZabkaPowerBI-IR'))]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		}
	]
}